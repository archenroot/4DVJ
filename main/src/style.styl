@import 'nib'

global-reset()

html, body
	width 100%
	height 100%
	overflow hidden
	background: black

#main
	position: absolute
	top: 0
	left: 0
	width 100%
	height 100%
	background black
	cursor none
	z-index: 99
	
#split
	position: absolute
	width: 33.333333%
	height: 100%
	top: 0
	left: 33.333333%
	border: 1px solid #444444